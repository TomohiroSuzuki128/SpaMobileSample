<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <EditText android:id="@+id/fromZipCodeTextEdit" android:layout_width="match_parent" android:layout_height="wrap_content" android:hint="@string/hint_zipcode" android:inputType="number"/>
    <Button android:id="@+id/fromZipCodeButton" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/from_zipcode" />
    <Spinner android:id="@+id/fromAddressSpinner" android:layout_width="match_parent" android:layout_height="wrap_content" />
    <EditText android:id="@+id/fromAddressTextEdit" android:layout_width="match_parent" android:layout_height="wrap_content" android:hint="@string/hint_address1"/>
    <EditText android:layout_width="match_parent" android:layout_height="wrap_content" android:hint="@string/hint_address2"/>
</LinearLayout>
